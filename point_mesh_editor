from ursina import *
from ursina.prefabs.first_person_controller import FirstPersonController
import itertools
import numpy as np
from scipy.spatial import Delaunay

app = Ursina()


class Cons:
    entities = []
    points = None
    edit_mode = True
    build_mode = not edit_mode


class meshGrid():
    def __init__(self, size):
        self.size = size
        self.points = []
        for i in range(self.size):
            temp = []
            for j in range(self.size):
                temp.append(Vec3(i, 0, j))
            self.points.append(temp)


class MeshPoint(Entity):
    def __init__(self, position, index):
        super().__init__(
            model='cube',
            double_sided=True,
            position=position,
            color=color.blue,
            scale=0.3,
            collider='box',
            visible=True
        )
        self.index = index
        self.i = 0
        self.update_interval = 30

    def update(self):
        if Cons.edit_mode == True:
            if mouse.left and self.hovered:
                self.change_height_up()
                # print(self.index)
                self.get_others(3, 'u')
                mesh_update()

            if mouse.right and self.hovered:
                self.change_height_down()
                self.get_others(3, 'd')
                mesh_update()

        if Cons.build_mode == True and Cons.edit_mode == False:
            if mouse.left and self.hovered:
                position = np.add(self.position, (0, .5, 0))
                b = Building(position=position)

            if mouse.right and self.hovered:
                pass

    def change_height_up(self):
        self.position += Vec3(0, 0.1, 0)
        print(self.position)
        print(self.index)

    def change_height_down(self):
        self.color = color.blue
        self.position += Vec3(0, -0.1, 0)
        # print(self.index)

    def get_others(self, depth, up_down):
        index = self.index
        for i in range(depth):
            iter = itertools.product(range(-i, i + 1), repeat=2)
            #    iter = list(iter)
            for it in iter:
                con = np.add(index, it)
                # print(con)
                if 0 <= int(con[0]) < len(Cons.entities) and 0 <= int(con[1]) < len(Cons.entities):
                    e = Cons.entities[int(con[0])][int(con[1])]
                    if up_down == 'u':
                        e.position += Vec3(0, 0.1, 0)
                        e.color = color.green
                    if up_down == 'd':
                        e.position += Vec3(0, -0.1, 0)
                        e.color = color.red

class Building(Entity):
    def __init__(self, position):
        super().__init__(
            model='cube',
            position=position,
            color=color.white,
            scale=1,
            collider='box',
            visible=True
        )


def setup():
    mesh = meshGrid(40)
    # print(mesh.points)

    for i in mesh.points:
        temp = []
        for j in i:
            # print(i)
            e = MeshPoint(position=j, index=(j[0], j[2]))
            temp.append(e)
        Cons.entities.append(temp)

    verts = [vert for line in Cons.entities for vert in line]
    Cons.meshpoints = list(vert.position for vert in verts)
    points2 = list((i[0], i[2]) for i in Cons.meshpoints)
    Cons.tri = Delaunay(points2)
    print(Cons.tri.simplices)
    tri = tuple((tri[0], tri[1], tri[2]) for tri in Cons.tri.simplices)
    Cons.tris = tri
    print(Cons.tris)
    # for i in Cons.tris:
    #     print(i)
