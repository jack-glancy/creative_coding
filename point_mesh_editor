    # for i in tri:
    #     print(i)
    Cons.points = Entity(model=Mesh(vertices=Cons.meshpoints, triangles=Cons.tris, mode='line', thickness=1), color=color.black)

    sky = Sky()
    Cons.fpc = FirstPersonController(gravity=0, speed=5)


def input(key):
    if key == 'x':
        exit()
    if key == 'j':
        Cons.edit_mode = not Cons.edit_mode
    if key == 'shift':
        Cons.fpc.y -= held_keys['shift']
    if key == 'space':
        Cons.fpc.y += held_keys['space']
    if key == 'v':
        for line in Cons.entities:
            for e in line:
                e.visible = not e.visible
    if key == 'n':
        Cons.fpc.speed += 1
    if key == 't':
        Cons.fpc.speed -= 1
    if key == 'b':
        Cons.build_mode = not Cons.build_mode
        Cons.edit_mode = False


def mesh_update():
    verts = [vert for line in Cons.entities for vert in line]
    destroy(Cons.points)
    points = list(vert.position for vert in verts)
    Cons.points = Entity(model=Mesh(vertices=points, triangles=Cons.tris, mode='line', thickness=1), color=color.black)

setup()
app.run()
